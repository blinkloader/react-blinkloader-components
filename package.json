{
  "name": "@blinkloader/react-blinkloader-components",
  "version": "2.0.6",
  "description": "React components for Blinkloader",
  "main": "./lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "test": "mocha",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:umd": "cross-env BABEL_ENV=commonjs NODE_ENV=development webpack src/index.js dist/react-blinkloader-components.js --config webpack.config.prod.js",
    "build:umd:min": "cross-env BABEL_ENV=commonjs NODE_ENV=production webpack src/index.js dist/react-blinkloader-components.min.js --config webpack.config.prod.js",
    "build": "yarn run build:commonjs && yarn run build:es && yarn run build:umd && yarn run build:umd:min",
    "lint": "eslint src demo",
    "clean": "rimraf lib dist es",
    "prettier": "prettier src/**/*.js demo/**/*.js --write",
    "prettier-list-different": "prettier src/**/*.js demo/**/*.js --list-different",
    "prepublish": "yarn run clean && yarn run build",
    "demo": "webpack-dev-server --content-base dist",
    "doctoc": "doctoc README.md"
  },
  "keywords": [],
  "author": "Blinkloader (https://blinkloader.com)",
  "license": "Apache 2.0",
  "dependencies": {
    "dotenv-webpack": "^1.6.0",
    "prop-types": "^15.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blinkloader/react-blinkloader-components.git"
  },
  "files": [
    "dist",
    "lib",
    "src",
    "es"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "doctoc": "^1.4.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.10.0",
    "eslint-config-stripe": "^1.2.0",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.27.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^23.6.0",
    "jest-cli": "^22.4.4",
    "jsdom": "^11.12.0",
    "mocha": "^5.2.0",
    "prettier": "^1.15.3",
    "react": "^16.7.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": ">=16.3.3",
    "react-test-renderer": "^16.7.0",
    "rimraf": "^2.6.3",
    "sinon": "^4.5.0",
    "sync-request": "^6.0.0",
    "webpack": "^3.12.0",
    "webpack-dev-server": ">=3.1.11"
  }
}
